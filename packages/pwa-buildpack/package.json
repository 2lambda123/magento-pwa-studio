{
  "name": "@magento/anhinga",
  "version": "0.1.0",
  "description": "Build/Layout optimization tooling for the Magento PWA",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --ignore __tests__ --source-maps",
    "test": "run-p jest:ci prettier:check",
    "test:dev": "jest --watch",
    "jest:ci": "jest -i --forceExit --testResultsProcessor=./node_modules/jest-junit-reporter",
    "prettier": "prettier --write 'src/**/*.js'",
    "prettier:check": "prettier-check 'src/**/*.js'"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magento-research/anhinga.git"
  },
  "keywords": [
    "magento",
    "pwa",
    "babel",
    "webpack"
  ],
  "author": "Magento Commerce",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/magento-research/anhinga/issues"
  },
  "homepage": "https://github.com/magento-research/anhinga#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "dedent": "^0.7.0",
    "jest": "^21.2.1",
    "jest-junit-reporter": "^1.1.0",
    "memory-fs": "^0.4.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.8.1",
    "prettier-check": "^2.0.0",
    "react": "^16.1.1",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "loader-utils": "^1.1.0",
    "webpack-sources": "^1.1.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "__fixtures__"
    ]
  }
}
