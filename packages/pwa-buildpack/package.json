    "write-file-webpack-plugin": "~4.5.0",
    "yargs": "~13.2.2"
  },
  "devDependencies": {
    "@babel/cli": "~7.2.3",
    "@babel/plugin-proposal-class-properties": "~7.3.4",
    "dedent": "~0.7.0",
    "memory-fs": "~0.4.1",
    "rimraf": "~2.6.3",
    "sharp": "~0.21.2",
    "strip-ansi": "~5.0.0",
